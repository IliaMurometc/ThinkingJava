package edu.colorado.ch04.link;

public class IntLinkedBagTest {
	public static void main(String[] args){
		addAllTest();
	}
	
	public static void addAllTest(){
		IntLinkedBag bag1 = new IntLinkedBag();
		bag1.add(1);
		bag1.add(2);
		bag1.add(3);
		IntLinkedBag bag2 = new IntLinkedBag();
		bag1.add(4);
		bag1.add(5);
		bag1.add(6);
		bag1.addAll(bag2);
		System.out.println("addAllTest: " + bag1);
	}
}

